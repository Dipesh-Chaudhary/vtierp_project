# vtierp_project_custom/.env.example
GOOGLE_API_KEY="YOUR_GEMINI_API_KEY_HERE"
LOG_LEVEL="INFO"

# These can be set here or in docker-compose.yml,
# docker-compose.yml takes precedence if defined in both for container env.
# LLM Models (matching your notebook)
LLM_RAG_MODEL="gemini-2.5-flash-preview-05-20" 
LLM_AUX_MODEL="gemini-1.5-flash" # Same as above
EMBEDDINGS_MODEL="models/text-embedding-004"

# Processing Parameters (from your notebook, can be fine-tuned)
MAX_IMAGES_TO_LLM_FINAL=3
MAX_ELEMENTS_FOR_VLM_DESCRIPTION_PER_PDF=8
RENDER_DPI_PYMUPDF=150
MIN_VISUAL_WIDTH_PYMUPDF=30
MIN_VISUAL_HEIGHT_PYMUPDF=30
OCR_DPI=300